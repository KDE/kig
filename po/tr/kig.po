msgid "Osculating circle of a curve at a point"
msgstr "Bu noktadaki eğrinin dokunum eğrisi"

#, kde-format
msgid "Osculating circle of this curve"
msgstr "Bu eğrinin dokunum eğrisi"

#, kde-format
msgid "Osculating circle at this point"
msgstr "Bu noktanın dokunum eğrisi"

#, kde-format
msgid "Select the point..."
msgstr "Noktayı seç..."

#, kde-format
msgid "Square"
msgstr "Kare"

#, kde-format
